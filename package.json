{
  "name": "formsy-material-ui-react16",
  "version": "0.0.0-development",
  "description": "A formsy-react compatibility wrapper for Material-UI form components with React 16",
  "keywords": [
    "formsy",
    "formsy-react",
    "form",
    "MUI",
    "Material-UI",
    "React",
    "jsx",
    "react-component"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "formsy-react": "^1.1.4",
    "keycode": "^2.2.0",
    "material-ui": "^0.20.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "commitizen": "^2.10.1",
    "condition-circle": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.2.0",
    "husky": "^1.0.0-rc.13",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "semantic-release": "^15.9.3"
  },
  "scripts": {
    "build": "babel ./src -d ./lib",
    "prepublish": "npm run build",
    "lint": "eslint --ext .jsx,.js src test && echo \"eslint: no lint errors\"",
    "cz": "git-cz",
    "semantic-release": "semantic-release"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/debitech/formsy-material-ui-react16.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "release": {
    "branch": "master",
    "verifyConditions": "condition-circle"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
